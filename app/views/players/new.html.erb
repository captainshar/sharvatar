<h1>New Player</h1>

<%= form_for :player, url: players_path do |f| %>
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </p>
 
  <p>
    <%= f.label :bio %><br>
    <%= f.text_area :bio %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>